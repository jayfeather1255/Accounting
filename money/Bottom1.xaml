<UserControl x:Class="money.Bottom1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:money"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border Grid.Row="1" Background="White" Padding="10" CornerRadius="10" Width="60" Height="60" BorderBrush="LightSteelBlue" BorderThickness="2">
        </Border>
        <Rectangle Height="38" Width="8" Fill="DodgerBlue"></Rectangle>
        <Rectangle Height="8" Width="38" Fill="DodgerBlue" HorizontalAlignment="Center" VerticalAlignment="Center"></Rectangle>
    </Grid>
</UserControl>
